{
  "name": "wiseguide-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prestart": "npm run-script load-env",
    "start": "node ./bin/www",

    "load-env": "node -e \"require('node-env-file')('.env',{raise:false})\"",

    "premigrate": "npm run-script load-env",
    "migrate": "node migrate",

    "style-check": "./node_modules/jscs/bin/jscs ."
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "morgan": "~1.6.1",
    "node-env-file": "^0.1.7",
    "pg": "^4.4.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.10.0",
    "umzug": "^1.6.0"
  },
  "devDependencies": {
    "jscs": "^2.2.1"
  }
}
